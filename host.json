{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            },
            "enableLiveMetricsFilters": true
        }
    },
    "extensions": {
    "queues": {
      "messageEncoding": "none",
      "maxDequeueCount": 3,
      "visibilityTimeout": "00:00:30",
      "maxPollingInterval": "00:00:02"
    }
  }
}
